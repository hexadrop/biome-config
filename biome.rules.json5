{
	"$schema": "https://biomejs.dev/schemas/2.3.6/schema.json",
	"assist": {
		"actions": {
			"source": {
				"organizeImports": "on",
				"useSortedAttributes": "on",
				"useSortedKeys": "on",
				"useSortedProperties": "on"
			}
		},
		"enabled": true
	},
	"css": {
		"assist": {
			"enabled": true
		},
		"formatter": {
			"enabled": true,
			"quoteStyle": "single"
		},
		"linter": {
			"enabled": true
		},
		"parser": {
			"allowWrongLineComments": false,
			"cssModules": true,
			"tailwindDirectives": true,
		}
	},
	"formatter": {
		"attributePosition": "multiline",
		"bracketSameLine": true,
		"bracketSpacing": true,
		"enabled": true,
		"expand": "always",
		"formatWithErrors": false,
		"indentStyle": "tab",
		"indentWidth": 4,
		"lineEnding": "auto",
		"lineWidth": 120
	},
	"graphql": {
		"assist": {
			"enabled": true
		},
		"formatter": {
			"enabled": true,
			"quoteStyle": "single"
		},
		"linter": {
			"enabled": true
		}
	},
	"html": {
		"assist": {
			"enabled": true
		},
		"experimentalFullSupportEnabled": true,
		"formatter": {
			"enabled": true,
			"indentScriptAndStyle": true,
			"selfCloseVoidElements": "always",
			"whitespaceSensitivity": "css"
		},
		"linter": {
			"enabled": true
		},
		"parser": {
			"interpolation": true
		}
	},
	"javascript": {
		"assist": {
			"enabled": true
		},
		"formatter": {
			"arrowParentheses": "asNeeded",
			"enabled": true,
			"jsxQuoteStyle": "single",
			"operatorLinebreak": "after",
			"quoteProperties": "asNeeded",
			"quoteStyle": "single",
			"semicolons": "always",
			"trailingCommas": "none"
		},
		"jsxRuntime": "transparent",
		"linter": {
			"enabled": true
		},
		"parser": {
			"gritMetavariables": false,
			"jsxEverywhere": false,
			"unsafeParameterDecoratorsEnabled": false
		},
		"globals": [],
	},
	"json": {
		"assist": {
			"enabled": true
		},
		"formatter": {
			"enabled": true,
			"trailingCommas": "none"
		},
		"linter": {
			"enabled": true
		},
		"parser": {
			"allowComments": false,
			"allowTrailingCommas": false
		}
	},
